<div class="popover padding-medium slight-rounded single-sharp-corner absolute-child {{extraClass}}" 
    data-gallery-item-data="true" 
   {{#each dataset}} 
    {{concat "data-" (camelCaseToDashCase @key)}}="{{this}}"
   {{/each}}
    >
    {{> (dynamicPartial this.passedPartial)
        this.passedPartialContext
    }}
</div>